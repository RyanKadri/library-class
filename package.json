{
  "name": "library-class",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:web": "parcel packages/site/src/index.html",
    "start:backend": "npm run build:docker && npm run run:docker",
    "build:docker": "docker build -t rk0-backend/games ./packages/games-backend",
    "run:docker": "docker run -p 5000:5000 -it rk0-backend/games",
    "build": "rm -rf dist/site && parcel build packages/site/src/index.html -d dist/site && ./scripts/monaco.sh",
    "test": "echo \"Error: no test specified\" && exit 1",
    "validate:cfn:template": "aws cloudformation validate-template --template-body file://./template.main.yml",
    "package:template": "aws cloudformation package --template-file ./template.main.yml --output-template-file ./dist/output.template.yml --s3-bucket cf-templates-k8vby7iizoq8-us-east-1",
    "deploy:template": "aws cloudformation deploy --template-file ./dist/output.template.yml --stack-name rk0-site --capabilities CAPABILITY_NAMED_IAM",
    "deploy:template:full": "npm run package:template && npm run deploy:template",
    "create:changeset": "aws cloudformation create-change-set --stack-name rk0-site --template-body file://./dist/output.template.yml --change-set-name rando --parameters file://./parameters.json --capabilities CAPABILITY_NAMED_IAM",
    "update:stack": "npm run package:template && aws cloudformation update-stack --stack-name rk0-site --template-body file://./dist/output.template.yml --capabilities CAPABILITY_NAMED_IAM --parameters file://./parameters.json"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.24",
    "@fortawesome/free-solid-svg-icons": "^5.11.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@material-ui/core": "^4.4.2",
    "@types/react-ga": "^2.3.0",
    "highlight.js": "^9.15.10",
    "monaco-editor": "^0.18.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-ga": "^2.6.0",
    "react-hammerjs": "^1.0.1",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "@types/highlight.js": "^9.12.3",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-hammerjs": "^1.0.1",
    "@types/react-router-dom": "^4.3.5",
    "marked": "^0.7.0",
    "parcel": "^1.12.3",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-bundle-visualiser": "github:gregtillbrook/parcel-plugin-bundle-visualiser#master",
    "sass": "^1.22.12",
    "typescript": "^3.6.3"
  }
}
